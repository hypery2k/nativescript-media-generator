{
  "name": "nativescript-media-generator",
  "version": "0.1.1",
  "description": "Generates NativeScript Media (Splashscreens, Icons,Images & Screenshots)",
  "main": "tns-media-gen.js",
  "scripts": {
    "build": "npm run eslint",
    "test": "jasmine-node  --captureExceptions --color --junitreport --output dist/reports/ test/specs/",
    "eslint": "eslint \"*.js\""
  },
  "nativescript": {
    "platforms": {
      "ios": "1.1.0",
      "android": "1.1.0"
    }
  },
  "dependencies": {
    "async": "2.1.4",
    "filewalker": "0.1.2",
    "fs": "0.0.2",
    "gm": "1.22.0",
